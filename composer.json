{
    "name": "tighten/symposium",
    "description": "A web app for conference speakers to track talks, bios, and conferences.",
    "keywords": [
        "laravel",
        "conference",
        "speakers",
        "talks"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Matt Stauffer",
            "email": "matt@tighten.co"
        },
        {
            "name": "Andrew Morgan",
            "email": "andrew.morgan@tighten.co"
        }
    ],
    "homepage": "https://symposiumapp.com/",
    "type": "project",
    "require": {
        "php": "^8.3",
        "algolia/algoliasearch-client-php": "^3.3",
        "blade-ui-kit/blade-icons": "^1.8",
        "cknow/laravel-money": "^8.4",
        "creativeorange/gravatar": "^1.0",
        "doctrine/dbal": "^3.9",
        "erusev/parsedown": "^1.7",
        "filament/filament": "^3.3",
        "guzzlehttp/guzzle": "^7.9",
        "intervention/image": "^2.7",
        "laravel/framework": "^12.21",
        "laravel/passport": "^12.4",
        "laravel/scout": "^10.16",
        "laravel/slack-notification-channel": "^3.6",
        "laravel/socialite": "^5.21",
        "laravel/tinker": "^2.10.1",
        "laravel/ui": "^4.6",
        "lcobucci/jwt": "^5.5",
        "league/flysystem-aws-s3-v3": "^3.29",
        "livewire/livewire": "^3.6",
        "predis/predis": "^1.1",
        "recaptcha/php5": "^2.0",
        "spatie/laravel-flare": "^1.1",
        "staudenmeir/eloquent-has-many-deep": "^1.21",
        "symfony/http-client": "^7.2",
        "symfony/intl": "^7.2",
        "symfony/mailgun-mailer": "^7.2"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.16",
        "fakerphp/faker": "^1.23",
        "mockery/mockery": "^1.6",
        "nunomaduro/collision": "^8.6",
        "phpunit/phpunit": "^11.5.3",
        "tightenco/duster": "^3.0",
        "laravel/pail": "^1.2.2"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        },
        "files": [
            "app/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-install-cmd": [
            "@php artisan clear-compiled",
            "@php artisan cache:clear"
        ],
        "post-update-cmd": [
            "@php artisan clear-compiled",
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force",
            "@php artisan cache:clear",
            "@php artisan filament:upgrade"
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi",
            "@php artisan filament:upgrade"
        ],
        "test": "phpunit",
        "dev": [
            "Composer\\Config::disableProcessTimeout",
            "npx concurrently -c \"#93c5fd,#c4b5fd,#fb7185,#fdba74\" \"php artisan serve\" \"php artisan queue:listen --tries=1\" \"php artisan pail --timeout=0\" \"npm run dev\" --names=server,queue,logs,vite"
        ]
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
