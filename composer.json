{
    "name": "tighten/symposium",
    "description": "A web app for conference speakers to track talks, bios, and conferences.",
    "keywords": [
        "laravel",
        "conference",
        "speakers",
        "talks"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Matt Stauffer",
            "email": "matt@tighten.co"
        },
        {
            "name": "Andrew Morgan",
            "email": "andrew.morgan@tighten.co"
        }
    ],
    "homepage": "https://symposiumapp.com/",
    "type": "project",
    "require": {
        "php": "^8.1",
        "algolia/algoliasearch-client-php": "^3.3",
        "blade-ui-kit/blade-icons": "^1.4",
        "bugsnag/bugsnag-laravel": "^2.24",
        "cknow/laravel-money": "^7.0",
        "creativeorange/gravatar": "^1.0",
        "doctrine/dbal": "^3.4",
        "erusev/parsedown": "^1.7",
        "geoip/geoip": "^1.17",
        "guzzlehttp/guzzle": "^7.2",
        "intervention/image": "^2.7",
        "laravel/framework": "^9.47",
        "laravel/passport": "^10.4",
        "laravel/scout": "^9.4",
        "laravel/slack-notification-channel": "^2.4",
        "laravel/socialite": "^5.5",
        "laravel/tinker": "^2.7",
        "laravel/ui": "^3.4",
        "laravelcollective/html": "^6.3",
        "lcobucci/jwt": "^4.2",
        "league/flysystem-aws-s3-v3": "^3.5",
        "pragmarx/firewall": "^2.3",
        "predis/predis": "^1.1",
        "recaptcha/php5": "^2.0",
        "symfony/http-client": "^6.0",
        "symfony/intl": "^6.0",
        "symfony/mailgun-mailer": "^6.0",
        "thujohn/twitter": "^3.1"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.7",
        "beyondcode/laravel-dump-server": "^1.8",
        "fakerphp/faker": "^1.9.1",
        "filp/whoops": "^2.14",
        "mockery/mockery": "^1.4.4",
        "nunomaduro/collision": "^6.3",
        "phpunit/phpunit": "^9.5.10",
        "spatie/laravel-ignition": "^1.5",
        "tightenco/duster": "^0.3.2"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        },
        "files": [
            "app/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-install-cmd": [
            "@php artisan clear-compiled",
            "@php artisan cache:clear"
        ],
        "post-update-cmd": [
            "@php artisan clear-compiled",
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force",
            "@php artisan cache:clear"
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "test": "phpunit"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
