{
    "name": "tighten/symposium",
    "description": "A web app for conference speakers to track talks, bios, and conferences.",
    "keywords": [
        "laravel",
        "conference",
        "speakers",
        "talks"    
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Matt Stauffer",
            "email": "matt.stauffer@tighten.co"
        }
    ],
    "homepage": "https://symposiumapp.com/",
    "type": "project",
    "require": {
        "php": "^7.4|^8.0",
        "algolia/algoliasearch-client-php": "^3.2",
        "blade-ui-kit/blade-icons": "^1.2",
        "bugsnag/bugsnag-laravel": "^2.23",
        "doctrine/dbal": "^3.3",
        "erusev/parsedown": "^1.7",
        "fruitcake/laravel-cors": "^2.0",
        "geoip/geoip": "^1.17",
        "guzzlehttp/guzzle": "^7.0.1",
        "intervention/image": "^2.7",
        "laravel/framework": "^8.83",
        "laravel/passport": "^10.3",
        "laravel/scout": "^9.4",
        "laravel/slack-notification-channel": "^2.4",
        "laravel/socialite": "^5.5",
        "laravel/tinker": "^2.5",
        "laravel/ui": "^3.4",
        "laravelcollective/html": "^6.3",
        "lcobucci/jwt": "^4.1",
        "pragmarx/firewall": "^2.3",
        "predis/predis": "^1.1",
        "recaptcha/php5": "^2.0",
        "thomaswelton/laravel-gravatar": "^1.3",
        "thujohn/twitter": "^3.1",
        "toin0u/geocoder-laravel": "^4.4"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.6",
        "beyondcode/laravel-dump-server": "^1.8",
        "filp/whoops": "^2.14",
        "mockery/mockery": "^1.4.4",
        "nunomaduro/collision": "^5.10",
        "phpunit/phpunit": "^9.5.10",
        "facade/ignition": "^2.5",
        "fakerphp/faker": "^1.9.1"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        },
        "files": [
            "app/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-install-cmd": [
            "@php artisan clear-compiled",
            "@php artisan cache:clear"
        ],
        "post-update-cmd": [
            "@php artisan clear-compiled",
            "@php artisan vendor:publish --tag=laravel-assets --ansi",
            "@php artisan cache:clear"
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "test": "phpunit"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
