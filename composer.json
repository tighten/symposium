{
    "name": "tighten/symposium",
    "description": "A web app for conference speakers to track talks, bios, and conferences.",
    "keywords": [
        "laravel",
        "conference",
        "speakers",
        "talks"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Matt Stauffer",
            "email": "matt@tighten.co"
        },
        {
            "name": "Andrew Morgan",
            "email": "andrew.morgan@tighten.co"
        }
    ],
    "homepage": "https://symposiumapp.com/",
    "type": "project",
    "require": {
        "php": "^8.3",
        "algolia/algoliasearch-client-php": "^3.3",
        "awcodes/drop-in-action": "^0.2.0",
        "blade-ui-kit/blade-icons": "^1.6",
        "bugsnag/bugsnag-laravel": "^2.27",
        "cknow/laravel-money": "^8.0",
        "creativeorange/gravatar": "^1.0",
        "doctrine/dbal": "^3.6",
        "erusev/parsedown": "^1.7",
        "filament/filament": "^3.2",
        "geoip/geoip": "^1.17",
        "guzzlehttp/guzzle": "^7.2",
        "intervention/image": "^2.7",
        "laravel/framework": "^11.16",
        "laravel/passport": "^12.0",
        "laravel/scout": "^10.8",
        "laravel/slack-notification-channel": "^3.2",
        "laravel/socialite": "^5.11",
        "laravel/tinker": "^2.9",
        "laravel/ui": "^4.4",
        "laravelcollective/html": "^6.4",
        "lcobucci/jwt": "^4.3",
        "league/flysystem-aws-s3-v3": "^3.12",
        "livewire/livewire": "^3.3",
        "pragmarx/firewall": "^2.3",
        "predis/predis": "^1.1",
        "recaptcha/php5": "^2.0",
        "staudenmeir/eloquent-has-many-deep": "^1.20",
        "symfony/http-client": "^7.0",
        "symfony/intl": "^7.0",
        "symfony/mailgun-mailer": "^7.0",
        "thujohn/twitter": "^3.3"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.10",
        "beyondcode/laravel-dump-server": "^2.0",
        "fakerphp/faker": "^1.23",
        "mockery/mockery": "^1.6",
        "nunomaduro/collision": "^8.0",
        "phpunit/phpunit": "^11.0.1",
        "tightenco/duster": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        },
        "files": [
            "app/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-install-cmd": [
            "@php artisan clear-compiled",
            "@php artisan cache:clear"
        ],
        "post-update-cmd": [
            "@php artisan clear-compiled",
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force",
            "@php artisan cache:clear",
            "@php artisan filament:upgrade"
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "test": "phpunit"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
